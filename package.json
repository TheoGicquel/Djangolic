{
  "name": "djangolic",
  "version": "1.0.0",
  "description": "A django beverage crud website",
  "main": "index.js",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "venv-unix": "source venv/bin/activate",
    "venv-win": "venv\\Scripts\\activate",
    "start": "python manage.py runserver",
    "dev-tailwind": "python manage.py tailwind start",
    "dev-tailwind-build": "python manage.py tailwind build",
    "dev-runserver": "python manage.py runserver",
    "run-dev": "npm run dev-tailwind && npm run dev-runserver",
    "test": "echo \"Error: no test specified\" && exit 1",
    "env-gen": "touch djangolic/.env;cat djangolic/.env.example > djangolic/.env"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/theogicquel/Djangolic.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/theogicquel/Djangolic/issues"
  },
  "homepage": "https://github.com/theogicquel/Djangolic#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "environ": "^3.0.3",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4"
  },
  "dependencies": {
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.19",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "postcss-simple-vars": "^7.0.1",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "environ": "^3.0.3",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4"
  }
}
